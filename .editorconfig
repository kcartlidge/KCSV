# This is excluded from the solution items intentionally
# to avoid it being updated in full Visual Studio as that
# pollutes the file with extra items when doing that.
#
# Please make any changes manually in a text editor instead.

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
indent_size = 2

[*.{md,markdown}]
indent_style = space

[*.{cs,cshtml}]
indent_style = space
indent_size = 4
